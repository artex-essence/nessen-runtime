{
  "name": "nessen-runtime",
  "version": "1.1.0",
  "description": "Enterprise-grade HTTP runtime for Node.js. Zero dependencies, production-ready with 100% security confidence. 45k+ req/sec, 8/8 tests passing, comprehensive audit verified.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:examples": "tsc -p tsconfig.examples.json",
    "start": "node dist/server.js",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf dist dist-test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test --ext .ts",
    "verify": "npm run typecheck && npm run build && npm run lint",
    "audit": "npm audit --production",
    "test": "npm run verify && tsc -p tsconfig.test.json && node test-runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artex-essence/nessen-runtime.git"
  },
  "keywords": [
    "http",
    "runtime",
    "server",
    "middleware",
    "performance",
    "security",
    "typescript",
    "nodejs",
    "enterprise",
    "production-ready",
    "zero-dependencies",
    "high-performance",
    "secure",
    "graceful-shutdown",
    "structured-logging"
  ],
  "author": "Artex Agency Inc",
  "license": "Apache-2.0",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": {
    "url": "https://github.com/artex-essence/nessen-runtime/issues"
  },
  "homepage": "https://github.com/artex-essence/nessen-runtime#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.9.3"
  }
}